{
  "include": [
    "./src/**/*.ts"
  ],
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "removeComments": true,
    "resolveJsonModule": true,
    "sourceMap": true
  }
}